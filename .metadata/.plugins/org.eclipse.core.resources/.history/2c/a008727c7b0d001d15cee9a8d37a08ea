package com.Demo_Array;

public class Array_Practise {
	public static void main(String[] args) {
		
		int a[]= {10,20,30,90,10,10};
		int length = a.length;
		System.out.println(length);
		for(int i =0;i<=a.length-1;i++) {
			
		
		if(a[i]==10) {
			a[i]=100;
		}
		System.out.println(a[i]);
		}
	}
	
}
